	<% for period in @periods -%><% unless period.blank? %><period value="<%= @range.shift %>">
			<% for transfer in period -%><transfer agent_name="<%= transfer.agent.name %>" recipient_name="<%= transfer.recipient.name %>" lon="<%= transfer.agent.lon %>" lat="<%= transfer.agent.lat %>" value="<%= transfer.value %>" />
			<% end -%>
	</period>
	<% end -%><% end -%>